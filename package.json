{
  "name": "nudeadmin",
  "version": "0.1.0",
  "description": "Administrative dashboard for NudeForge / NudeFlow platform (moderation, settings, media management).",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node --watch src/app.js",
    "test": "vitest run -c ../vitest.config.mjs",
    "test:watch": "vitest -c ../vitest.config.mjs --watch",
    "coverage": "vitest run -c ../vitest.config.mjs --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
  "ejs": "^3.1.10",
  "express": "^4.19.2",
  "express-session": "^1.18.2",
  "sharp": "^0.33.5"
  },
  "devDependencies": {
    "vitest": "^1.6.0",
    "eslint": "^9.9.0",
    "eslint-plugin-vitest": "^0.5.4"
  }
}
